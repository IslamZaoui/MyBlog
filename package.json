{
	"name": "myblog",
	"version": "1.6.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.7.0",
		"@skeletonlabs/skeleton": "^2.9.1",
		"@skeletonlabs/tw-plugin": "0.4.0",
		"@sveltejs/adapter-vercel": "^5.3.0",
		"@sveltejs/kit": "^2.5.6",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.12",
		"@types/node": "20.12.7",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"autoprefixer": "10.4.19",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.37.0",
		"postcss": "8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.15",
		"svelte-check": "^3.6.9",
		"tailwindcss": "3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.9",
		"vite-plugin-tailwind-purgecss": "0.3.1"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "1.6.3",
		"@formkit/auto-animate": "^0.8.2",
		"@inlang/paraglide-js-adapter-sveltekit": "^0.6.7",
		"@supabase/supabase-js": "^2.42.5",
		"@vercel/analytics": "^1.2.2",
		"@vercel/og": "^0.6.2",
		"gsap": "^3.12.5",
		"highlight.js": "^11.9.0",
		"just-camel-case": "^6.2.0",
		"lucide-svelte": "^0.372.0",
		"mdsvex": "^0.11.0",
		"mdsvex-reading-time": "^1.0.4",
		"plyr": "^3.7.8",
		"remark-gfm": "^4.0.0",
		"remark-unwrap-images": "^4.0.0",
		"satori-html": "^0.3.2",
		"svelte-youtube-embed": "^0.2.3",
		"unist-util-visit": "^5.0.0",
		"vite-imagetools": "^7.0.1"
	}
}
